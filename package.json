{
  "name": "vaulted",
  "version": "2.1.1",
  "description": "A nodejs wrapper library for using HashiCorp's Vault",
  "main": "./lib/vaulted.js",
  "files": [
    "lib",
    "config"
  ],
  "scripts": {
    "start": "npm rebuild && node ./lib/vaulted.js",
    "test": "mocha -R spec tests/**",
    "coverage": "istanbul cover _mocha -- tests/**",
    "test-watch": "mocha --growl -R spec -w tests/**/*.js",
    "docs": "gulp docs",
    "lint": "gulp lint"
  },
  "keywords": [
    "vault",
    "token",
    "auth",
    "secrets",
    "hashicorp"
  ],
  "author": "Christopher 'Chief' Najewicz <chief@beefdisciple.com>",
  "contributors": [{
      "name": "Kenny Jones",
      "email": "kenjones@cisco.com"
    }],
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.0.0",
    "confab-addons": "kenjones-cisco/confab-addons#master",
    "lodash": "^3.10.0",
    "request-promise": "^1.0.2",
    "request-debug": "^0.2.0",
    "js-yaml": "^3.4.6"
  },
  "devDependencies": {
    "jshint": "^2.8.0",
    "chai": "^3.0.0",
    "chai-as-promised": "^5.1.0",
    "istanbul": "^0.4.1",
    "mocha": "^2.2.5",
    "gulp": "^3.9.0",
    "gulp-jsdoc-to-markdown": "^1.2.0",
    "gulp-util": "^3.0.7",
    "gulp-rename": "^1.2.2",
    "gulp-jshint": "^2.0.0",
    "gulp-istanbul": "^0.10.3",
    "gulp-mocha": "^2.2.0"
  },
  "repository": "https://github.com/chiefy/vaulted"
}
